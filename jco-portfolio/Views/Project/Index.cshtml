@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model List<Portfolio.Models.Project>

<h2>Top 3 Starred Repos from my GitHub</h2>
<hr />

@foreach (var project in Model)
{
<h2>Project Name: @project.full_name</h2>
<h2>Link: <a href="@project.html_url">@project.html_url</a></h2>

}

